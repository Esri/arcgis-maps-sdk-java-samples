<?import com.esri.arcgisruntime.mapping.view.MapView?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import com.esri.arcgisruntime.mapping.view.MapView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.paint.Color?>
<StackPane fx:controller="com.esri.samples.displayinformation.sketch_on_map.SketchOnMapController" xmlns:fx="http://javafx.com/fxml"
           stylesheets="/css/style.css">
    <MapView fx:id="mapView"/>
    <VBox StackPane.alignment="TOP_RIGHT" maxWidth="255" maxHeight="110" styleClass="panel-region" spacing="6"
          alignment="CENTER">
        <padding>
            <Insets topRightBottomLeft="10"/>
        </padding>
        <background>
            <Background>
                <fills>
                    <BackgroundFill>
                        <fill>
                            <Color opacity="0.3"/>
                        </fill>
                    </BackgroundFill>
                </fills>
            </Background>
        </background>


    </VBox>




</StackPane>
